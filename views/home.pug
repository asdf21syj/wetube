//-pug는 템플릿 언어, express의 view engine
    HTML - <p>hello</p>
    extends 작성된 경로를 사용(확장)한다.

extends layouts/main
include mixins/videoBlock


block content
    .videos
        each video in videos 
            +videoBlock({
                title:video.title,
                views:video.views,
                videoFile:video.videoFile
            })
